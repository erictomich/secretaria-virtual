// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
  binaryTargets   = ["native"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Mutirao {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  nome          String
  descricao     String
  objetivos     String
  responsavel   String
  timestamp     String
  data_mutirao  String
  requisitos    String
  local         String
  participantes String
  concluido     Boolean
}

model FardadosAtivos {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  nome_completo String
  descricao     String
  objetivos     String
  responsavel   String
  timestamp     String
  data_mutirao  String
  requisitos    String
  local         String
  participantes String
  concluido     Boolean
}
